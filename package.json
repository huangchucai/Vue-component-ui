{
  "name": "vue-component-ui",
  "version": "0.0.4",
  "description": "Vue的常见ui组件",
  "main": "dist/index.js",
  "scripts": {
    "dev-test": "parcel watch test/* --no-cache & karma start",
    "test": "parcel build test/* --no-cache --no-minify && karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huangchucai/Vue-component-ui.git"
  },
  "keywords": [
    "Vue",
    "js",
    "component"
  ],
  "author": "sunnyhuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huangchucai/Vue-component-ui/issues"
  },
  "homepage": "https://github.com/huangchucai/Vue-component-ui#readme",
  "dependencies": {
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "ghooks": "^2.0.4",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^10.2.0",
    "parcel-bundler": "^1.12.3",
    "sinon": "^7.3.0",
    "sinon-chai": "^3.3.0",
    "stylus": "^0.54.5",
    "validate-commit-msg": "^2.14.0",
    "vue-template-compiler": "^2.5.16"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
